<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:tns="http://customeAlarmV2.types.malima.francetelecom.com" xmlns="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://customeAlarmV2.types.malima.francetelecom.com" elementFormDefault="qualified">

  <annotation>
    <documentation>
      This schema defines the service customer alarm services types.
    </documentation>
  </annotation>

  <!-- Create Trigger request input parameters -->
  <group name="CreateTriggerIn">
    <sequence>
      <!-- scope -->
      <element minOccurs="1" maxOccurs="1" name="scope" type="tns:Scope" />
      <!-- Trigger criteria -->
      <element minOccurs="1" maxOccurs="1" name="triggerCriteria" type="tns:TriggerCriteria" />
      <!-- notification -->
      <element minOccurs="1" maxOccurs="1" name="notification" type="boolean" />
      <!-- level -->
      <element minOccurs="1" maxOccurs="1" name="level" type="tns:Level" />
      <!-- groupId -->
      <element minOccurs="0" maxOccurs="1" name="groupId" type="tns:GroupId" />
      <!-- comment -->
      <element minOccurs="0" maxOccurs="1" name="comment" type="tns:TriggerComment" />
      <!-- notification by mail -->
      <element minOccurs="0" maxOccurs="1" name="notificationByMail" type="boolean" />
	  <!-- Business Rules -->
      <element minOccurs="0" maxOccurs="1" name="businessRules" type="tns:BusinessRules" />
    </sequence>
  </group>

  <!-- scope -->
  <complexType name="Scope">
    <choice>
      <!-- Line identifiers -->
      <element minOccurs="1" maxOccurs="1" name="lineIdentifier" type="tns:LineIdentifier" />
      <!-- customer environment number -->
      <element minOccurs="1" maxOccurs="1" name="customerEnvironnementNumber" type="tns:CustomerEnvironmentNumber" />
    </choice>
  </complexType>

  <!-- TriggerCriteria -->
  <complexType name="TriggerCriteria">
    <choice>
      <element minOccurs="1" maxOccurs="1" name="trafficTimeStampCriteria" type="tns:TrafficTimeStampCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficLocationCriteria" type="tns:TrafficLocationCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficGlobalThresholdCriteria" type="tns:TrafficGlobalThresholdCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficUnitaryThresholdCriteria" type="tns:TrafficUnitaryThresholdCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficBearerCriteria" type="tns:TrafficBearerCriteria" />
      <element minOccurs="1" maxOccurs="1" name="updateImeiCriteria" type="tns:UpdateImeiCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficLazyMachineCriteria" type="tns:TrafficLazyMachineCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficSilentMachineCriteria" type="tns:TrafficSilentMachineCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficCrazyMachineCriteria" type="tns:TrafficCrazyMachineCriteria" />
      <element minOccurs="1" maxOccurs="1" name="subscriptionStatusChangeCriteria" type="tns:SubscriptionStatusChangeCriteria" />
      <element minOccurs="1" maxOccurs="1" name="subscriptionOptionChangeCriteria" type="tns:SubscriptionOptionChangeCriteria" />
    </choice>
  </complexType>

  <!-- Traffic time stamp criteria -->
  <complexType name="TrafficTimeStampCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="allowedPeriods" type="tns:AllowedPeriods" />
    </sequence>
  </complexType>

  <!-- traffic location criteria -->
  <complexType name="TrafficLocationCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="allowedCountriesAndOperators" type="tns:AllowedCountriesAndOperators" />
    </sequence>
  </complexType>

  <!-- country -->
  <complexType name="Country">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="countryCode" type="tns:CountryCode" />
      <element minOccurs="0" maxOccurs="1" name="operators" type="tns:Operators" />
    </sequence>
  </complexType>

  <!-- country code -->
  <simpleType name="CountryCode">
    <restriction base="int">
      <minInclusive value="0" />
      <maxInclusive value="32767" />
    </restriction>
  </simpleType>

  <!-- operators -->
  <complexType name="Operators">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="operatorCode" type="tns:OperatorCode" />
    </sequence>
  </complexType>

  <!-- Operator code -->
  <simpleType name="OperatorCode">
    <restriction base="string">
      <maxLength value="5" />
    </restriction>
  </simpleType>

  <!-- TrafficGlobalThresholdCriteria -->
  <complexType name="TrafficGlobalThresholdCriteria">
    <choice>
      <element minOccurs="1" maxOccurs="1" name="roaming" type="tns:Roaming" />
      <element minOccurs="1" maxOccurs="1" name="local" type="tns:Local" />
      <element minOccurs="1" maxOccurs="1" name="allOrigin" type="tns:AllOrigin" />
    </choice>
  </complexType>

  <!-- TrafficUnitaryThresholdCriteria -->
  <complexType name="TrafficUnitaryThresholdCriteria">
    <choice>
      <element minOccurs="1" maxOccurs="1" name="roaming" type="tns:Roaming" />
      <element minOccurs="1" maxOccurs="1" name="local" type="tns:Local" />
      <element minOccurs="1" maxOccurs="1" name="allOrigin" type="tns:AllOrigin" />
    </choice>
  </complexType>

  <!-- trafficLazyMachineCriteria -->
  <complexType name="TrafficLazyMachineCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="value" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="bearer" type="tns:Bearer" />
      <element minOccurs="1" maxOccurs="1" name="callOrigin" type="tns:CallOrigin" />
      <element minOccurs="1" maxOccurs="1" name="period" type="int" />
    </sequence>
  </complexType>

  <!-- trafficSilentMachineCriteria -->
  <complexType name="TrafficSilentMachineCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="period" type="int" />
    </sequence>
  </complexType>

  <!-- trafficCrazyMachineCriteria -->
  <complexType name="TrafficCrazyMachineCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="value" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="bearer" type="tns:Bearer" />
      <element minOccurs="1" maxOccurs="1" name="callOrigin" type="tns:CallOrigin" />
      <element minOccurs="1" maxOccurs="1" name="period" type="int" />
    </sequence>
  </complexType>

  <!-- subscriptionStatusChangeCriteria -->
  <complexType name="SubscriptionStatusChangeCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="watchedM2MSubscriptionStatuses" type="tns:WatchedM2MSubscriptionStatuses" />
    </sequence>
  </complexType>
  <complexType name="WatchedM2MSubscriptionStatuses">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="status" type="string" />
    </sequence>
  </complexType>

  <!-- subscriptionOptionChangeCriteria -->
  <complexType name="SubscriptionOptionChangeCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="watchedSubscriptionOptions" type="tns:WatchedSubscriptionOptions" />
      <element minOccurs="1" maxOccurs="1" name="optionChangeTypes" type="tns:OptionChangeTypes" />
    </sequence>
  </complexType>
  <complexType name="WatchedSubscriptionOptions">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="option" type="string" />
    </sequence>
  </complexType>
  <complexType name="OptionChangeTypes">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="optionChangeType" type="string" />
    </sequence>
  </complexType>

  <!-- Roaming -->
  <complexType name="Roaming">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="trafficRestriction" type="tns:TrafficRestriction" />
    </sequence>
  </complexType>

  <!-- local -->
  <complexType name="Local">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="trafficRestriction" type="tns:TrafficRestriction" />
    </sequence>
  </complexType>
  <!-- local -->
  <complexType name="AllOrigin">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="trafficRestriction" type="tns:TrafficRestriction" />
    </sequence>
  </complexType>

  <!-- Traffic restriction -->
  <complexType name="TrafficRestriction">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="value" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="bearer" type="tns:Bearer" />
    </sequence>
  </complexType>

  <!-- TrafficBearerCriteria -->
  <complexType name="TrafficBearerCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="allowedBearers" type="tns:AllowedBearers" />
    </sequence>
  </complexType>

  <!-- AllowedBearers -->
  <complexType name="AllowedBearers">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="bearer" type="tns:Bearer" />
    </sequence>
  </complexType>

  <!-- update imei criteria -->
  <complexType name="UpdateImeiCriteria" />

  <!-- Create Trigger request output parameters -->
  <group name="CreateTriggerOut">
    <sequence>
      <!-- scope -->
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </group>

  <!-- Delete Trigger request intput parameters -->
  <group name="DeleteTriggerIn">
    <sequence>
      <!-- trigger identifier -->
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </group>

  <!-- Delete Trigger request output parameters -->
  <group name="DeleteTriggerOut">
    <sequence>
      <!-- trigger identifier -->
      <element minOccurs="1" maxOccurs="1" name="deletedTriggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </group>

  <!-- update trigger request input parameters -->
  <group name="UpdateTriggerIn">
    <sequence>
      <!-- trigger identifier -->
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
      <!-- Trigger criteria -->
      <element minOccurs="1" maxOccurs="1" name="triggerCriteria" type="tns:TriggerCriteria" />
      <!-- notification -->
      <element minOccurs="1" maxOccurs="1" name="notification" type="boolean" />
      <!-- level -->
      <element minOccurs="1" maxOccurs="1" name="level" type="tns:Level" />
      <!-- groupId -->
      <element minOccurs="0" maxOccurs="1" name="groupId" type="tns:GroupId" />
      <!-- comment -->
      <element minOccurs="0" maxOccurs="1" name="comment" type="tns:TriggerComment" />
      <!-- trigger status -->
      <element minOccurs="1" maxOccurs="1" name="status" type="tns:TriggerStatus" />
      <!-- notification by mail -->
      <element minOccurs="0" maxOccurs="1" name="notificationByMail" type="boolean" />
	  <!-- Business Rules -->
      <element minOccurs="0" maxOccurs="1" name="businessRules" type="tns:BusinessRules" />
    </sequence>
  </group>


  <!-- update trigger request input parameters -->
  <group name="UpdateTriggerOut">
    <sequence>
      <!-- trigger identifier -->
      <element minOccurs="1" maxOccurs="1" name="updatedTriggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </group>

  <!-- List triggers request input parameters -->
  <group name="ListTriggersIn">
    <sequence>
      <choice>
        <!-- trigger identifier -->
        <element minOccurs="1" maxOccurs="1" name="triggerIdentifiers" type="tns:TriggerIdentifiers" />
        <!-- line identifier -->
        <element minOccurs="1" maxOccurs="1" name="lineIdentifiers" type="tns:LineIdentifierCollection" />
        <!-- customer environment identifier -->
        <element minOccurs="1" maxOccurs="1" name="customerEnvironmentNumber" type="tns:CustomerEnvironmentNumber" />
      </choice>
      <!-- show all origin trigger -->
      <element minOccurs="0" maxOccurs="1" name="showAllOriginTrafficTriggers" type="boolean" />
      <!-- show machine triggers -->
      <element minOccurs="0" maxOccurs="1" name="showMachineTriggers" type="boolean" />
      <!-- show extended triggers -->
      <element minOccurs="0" maxOccurs="1" name="showExtendedTriggers" type="boolean" />
      <!-- show subscription status change trigger -->
      <element minOccurs="0" maxOccurs="1" name="showSubscriptionStatusChangeTriggers" type="boolean" />
      <!-- show subscription option change trigger -->
      <element minOccurs="0" maxOccurs="1" name="showSubscriptionOptionChangeTriggers" type="boolean" />
	  <!-- show business rules -->
      <element minOccurs="0" maxOccurs="1" name="showBusinessRules" type="boolean" />
    </sequence>
  </group>

  <!-- trigger identifiers -->
  <complexType name="TriggerIdentifiers">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="triggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </complexType>


  <!-- List triggers request output parameters -->
  <group name="ListTriggersOut">
    <sequence>
      <!-- trigger identifier -->
      <element minOccurs="0" maxOccurs="1" name="triggers" type="tns:Triggers" />
      <!-- Unknown triggers -->
      <element minOccurs="0" maxOccurs="1" name="unknownTriggers" type="tns:UnknownTriggers" />
      <!-- Unknown identifiers -->
      <element minOccurs="0" maxOccurs="1" name="unknownLineIdentifiers" type="tns:LineIdentifierCollection" />
    </sequence>
  </group>

  <!-- trigger list -->
  <complexType name="Triggers">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="trigger" type="tns:Trigger" />
    </sequence>
  </complexType>

  <!-- trigge -->
  <complexType name="Trigger">
    <sequence>
      <!-- trigger scope -->
      <element minOccurs="1" maxOccurs="1" name="scope" type="tns:TriggerScope" />
      <!-- trigger identifier -->
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
      <!-- trigger type -->
      <element minOccurs="1" maxOccurs="1" name="triggerType" type="tns:TriggerType" />
      <!-- trigger criteria -->
      <element minOccurs="1" maxOccurs="1" name="triggerCriteria" type="tns:TriggerCriteria" />
      <!-- notification -->
      <element minOccurs="1" maxOccurs="1" name="notification" type="boolean" />
      <!-- level -->
      <element minOccurs="1" maxOccurs="1" name="level" type="tns:Level" />
      <!-- groupId -->
      <element minOccurs="0" maxOccurs="1" name="groupId" type="tns:GroupId" />
      <!-- comment -->
      <element minOccurs="0" maxOccurs="1" name="comment" type="tns:TriggerComment" />
      <!-- trigger status -->
      <element minOccurs="1" maxOccurs="1" name="status" type="tns:TriggerStatus" />
      <!-- notification by mail -->
      <element minOccurs="0" maxOccurs="1" name="notificationByMail" type="boolean" />
	  <!-- Business Rules -->
      <element minOccurs="0" maxOccurs="1" name="businessRules" type="tns:BusinessRules" />
    </sequence>
  </complexType>

  <!-- trigger scope -->
  <complexType name="TriggerScope">
    <choice>
      <element minOccurs="1" maxOccurs="1" name="subscriptionNumber" type="string" />
      <element minOccurs="1" maxOccurs="1" name="customerEnvironmentNumber" type="tns:CustomerEnvironmentNumber" />
    </choice>
  </complexType>

  <!-- unknown trigger identifiers list -->
  <complexType name="UnknownTriggers">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="triggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </complexType>

  <!-- get alarms request input parameters -->
  <group name="GetAlarmsIn">
    <sequence>
      <!-- alarm identifiers -->
      <element minOccurs="1" maxOccurs="1" name="alarmIdentifiers" type="tns:AlarmIdentifiers" />
      <!-- show alarm status -->
      <element minOccurs="0" maxOccurs="1" name="showAlarmStatus" type="boolean" />
      <!-- show all origin traffic alarm -->
      <element minOccurs="0" maxOccurs="1" name="showAllOriginTrafficAlarms" type="boolean" />
      <!-- show machine traffic alarms -->
      <element minOccurs="0" maxOccurs="1" name="showMachineAlarms" type="boolean" />
      <!-- show extended triggers -->
      <element minOccurs="0" maxOccurs="1" name="showExtendedAlarms" type="boolean" />
      <!-- show Alarms on M2M subscription status change -->
      <element minOccurs="0" maxOccurs="1" name="showSubscriptionStatusChangeAlarms" type="boolean" />
      <!-- show Alarms on subscription option change -->
      <element minOccurs="0" maxOccurs="1" name="showSubscriptionOptionChangeAlarms" type="boolean" />
      <!-- show business rules -->
      <element minOccurs="0" maxOccurs="1" name="showBusinessRules" type="boolean" />	  
    </sequence>
  </group>

  <!-- alarm identifiers list -->
  <complexType name="AlarmIdentifiers">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="alarmIdentifier" type="tns:AlarmIdentifier" />
    </sequence>
  </complexType>

  <!-- get alarms request input parameters -->
  <group name="GetAlarmsOut">
    <sequence>
      <!-- alarms -->
      <element minOccurs="0" maxOccurs="1" name="alarms" type="tns:Alarms" />
      <element minOccurs="0" maxOccurs="1" name="unknownAlarmIdentifiers" type="tns:UnknownAlarmIdentifiers" />
    </sequence>
  </group>

  <!-- alarms list -->
  <complexType name="Alarms">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="alarm" type="tns:Alarm" />
    </sequence>
  </complexType>

  <!-- alarm definition -->
  <complexType name="Alarm">
    <sequence>
      <!-- Alarm identifier -->
      <element minOccurs="1" maxOccurs="1" name="alarmIdentifier" type="tns:AlarmIdentifier" />
      <!-- Creation date -->
      <element minOccurs="1" maxOccurs="1" name="creationDate" type="dateTime" />
      <!-- alarm level -->
      <element minOccurs="1" maxOccurs="1" name="level" type="tns:Level" />
      <!-- alarm status -->
      <element minOccurs="0" maxOccurs="1" name="alarmStatus" type="string" />
      <!-- alarm status comment -->
      <element minOccurs="0" maxOccurs="1" name="alarmStatusComment" type="string" />
      <!-- customer environment -->
      <element minOccurs="1" maxOccurs="1" name="customerEnvironmentNumber" type="tns:CustomerEnvironmentNumber" />
      <!-- subscription number -->
      <element minOccurs="0" maxOccurs="1" name="subscriptionNumber" type="string" />
      <!-- notification date -->
      <element minOccurs="0" maxOccurs="1" name="notificationDate" type="dateTime" />
      <!-- notification result code -->
      <element minOccurs="0" maxOccurs="1" name="notificationResultCode" type="int" />
      <!-- notified URL -->
      <element minOccurs="0" maxOccurs="1" name="notifiedURL" type="string" />
      <!-- notification message -->
      <element minOccurs="0" maxOccurs="1" name="notificationMessage" type="string" />
      <!-- mail notification destination email -->
      <element minOccurs="0" maxOccurs="1" name="mailNotificationAddress" type="string" />
      <!-- mail notification date -->
      <element minOccurs="0" maxOccurs="1" name="mailNotificationDate" type="dateTime" />
      <!-- mail notification result code -->
      <element minOccurs="0" maxOccurs="1" name="mailNotificationResultCode" type="int" />
      <!-- mail notification result message -->
      <element minOccurs="0" maxOccurs="1" name="mailNotificationResultMessage" type="string" />
      <choice>
        <element minOccurs="1" maxOccurs="1" name="nbAlarmExceeded" type="tns:NbAlarmExceeded" />
        <element minOccurs="1" maxOccurs="1" name="triggeredAlarm" type="tns:TriggeredAlarm" />
      </choice>
      <!-- Triggered Business Rules -->
      <element minOccurs="0" maxOccurs="1" name="businessRules" type="tns:TriggeredBusinessRules" />
    </sequence>
  </complexType>

  <!-- alarm exceeded -->
  <complexType name="NbAlarmExceeded">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="quota" type="int" />
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
    </sequence>
  </complexType>

  <!-- triggererd alarm -->
  <complexType name="TriggeredAlarm">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
      <element minOccurs="1" maxOccurs="1" name="notificationRequired" type="boolean" />
      <element minOccurs="0" maxOccurs="1" name="triggerComment" type="tns:TriggerComment" />
      <element minOccurs="0" maxOccurs="1" name="triggerGroupId" type="tns:GroupId" />
      <element minOccurs="1" maxOccurs="1" name="triggerType" type="tns:TriggerType" />
      <element minOccurs="1" maxOccurs="1" name="triggerCriteria" type="tns:AlarmTriggerCriteria" />
      <element minOccurs="0" maxOccurs="1" name="notificationByMailRequired" type="boolean" />
    </sequence>
  </complexType>

  <!-- triggered alarm type -->
  <complexType name="AlarmTriggerCriteria">
    <choice>
      <element minOccurs="1" maxOccurs="1" name="trafficTimeStampCriteria" type="tns:AlarmTrafficTimeStampCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficLocationCriteria" type="tns:AlarmTrafficLocationCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficUnitaryThresholdCriteria" type="tns:AlarmTrafficUnitaryThresholdCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficGlobalThresholdCriteria" type="tns:AlarmTrafficGlobalThresholdCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficBearerCriteria" type="tns:AlarmTrafficBearerCriteria" />
      <element minOccurs="1" maxOccurs="1" name="updateImeiCriteria" type="tns:AlarmUpdateImeiCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficLazyMachineCriteria" type="tns:AlarmTrafficLazyMachineCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficSilentMachineCriteria" type="tns:AlarmTrafficSilentMachineCriteria" />
      <element minOccurs="1" maxOccurs="1" name="trafficCrazyMachineCriteria" type="tns:AlarmTrafficCrazyMachineCriteria" />
      <element minOccurs="1" maxOccurs="1" name="subscriptionStatusChangeCriteria" type="tns:AlarmSubscriptionStatusChangeCriteria" />
      <element minOccurs="1" maxOccurs="1" name="subscriptionOptionChangeCriteria" type="tns:AlarmSubscriptionOptionChangeCriteria" />
    </choice>
  </complexType>

  <!-- triggered timestamp alarm -->
  <complexType name="AlarmTrafficTimeStampCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="firstEventDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="lastEventDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="eventCount" type="int" />
      <element minOccurs="1" maxOccurs="1" name="allowedPeriods" type="tns:AllowedPeriods" />
    </sequence>
  </complexType>

  <!-- triggered traffic location alarm -->
  <complexType name="AlarmTrafficLocationCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="firstEventDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="lastEventDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="eventCount" type="int" />
      <element minOccurs="1" maxOccurs="1" name="allowedCountriesAndOperators" type="tns:AllowedCountriesAndOperators" />
      <element minOccurs="1" maxOccurs="1" name="forbiddenCountriesAndOperators" type="tns:ForbiddenCountriesAndOperators" />
    </sequence>
  </complexType>

  <!-- triggered unitary thershold alarm -->
  <complexType name="AlarmTrafficUnitaryThresholdCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="bearerType" type="tns:Bearer" />
      <element minOccurs="1" maxOccurs="1" name="roaming" type="boolean" />
      <element minOccurs="0" maxOccurs="1" name="callOrigin" type="tns:CallOrigin" />
      <element minOccurs="1" maxOccurs="1" name="thresholdValue" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="triggeringValue" type="tns:positiveLong" />
    </sequence>
  </complexType>

  <!-- triggered traffic lazy machine alarm -->
  <complexType name="AlarmTrafficLazyMachineCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="bearerType" type="tns:Bearer" />
      <element minOccurs="1" maxOccurs="1" name="callOrigin" type="tns:CallOrigin" />
      <element minOccurs="1" maxOccurs="1" name="thresholdValue" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="triggeringValue" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="period" type="int" />
    </sequence>
  </complexType>

  <!-- triggered traffic silent machine alarm -->
  <complexType name="AlarmTrafficSilentMachineCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="period" type="int" />
    </sequence>
  </complexType>

  <!-- triggered traffic crazy machine alarm -->
  <complexType name="AlarmTrafficCrazyMachineCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="bearerType" type="tns:Bearer" />
      <element minOccurs="1" maxOccurs="1" name="callOrigin" type="tns:CallOrigin" />
      <element minOccurs="1" maxOccurs="1" name="thresholdValue" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="triggeringValue" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="period" type="int" />
    </sequence>
  </complexType>


  <!-- triggered global thershold alarm -->
  <complexType name="AlarmTrafficGlobalThresholdCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="bearerType" type="tns:Bearer" />
      <element minOccurs="1" maxOccurs="1" name="roaming" type="boolean" />
      <element minOccurs="0" maxOccurs="1" name="callOrigin" type="tns:CallOrigin" />
      <element minOccurs="1" maxOccurs="1" name="thresholdValue" type="tns:positiveLong" />
      <element minOccurs="1" maxOccurs="1" name="triggeringValue" type="tns:positiveLong" />
    </sequence>
  </complexType>

  <!-- triggered bearer alarm -->
  <complexType name="AlarmTrafficBearerCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="firstEventDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="lastEventDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="eventCount" type="int" />
      <element minOccurs="1" maxOccurs="1" name="forbiddenBearerTypes" type="tns:ForbiddenBearerTypes" />
      <element minOccurs="1" maxOccurs="1" name="allowedBearerTypes" type="tns:AllowedBearerTypes" />
    </sequence>
  </complexType>

  <!-- triggered M2M Subscription status change alarm -->
  <complexType name="AlarmSubscriptionStatusChangeCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="detectedTargetStatus" type="string" />
      <element minOccurs="1" maxOccurs="1" name="updateStatusDate" type="dateTime" />
    </sequence>
  </complexType>

  <!-- triggered M2M Subscription status change alarm -->
  <complexType name="AlarmSubscriptionOptionChangeCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="option" type="string" />
      <element minOccurs="1" maxOccurs="1" name="description" type="string" />
      <element minOccurs="1" maxOccurs="1" name="optionChangeType" type="string" />
      <element minOccurs="1" maxOccurs="1" name="updateOptionDate" type="dateTime" />
    </sequence>
  </complexType>

  <!-- foridden bearer type list -->
  <complexType name="ForbiddenBearerTypes">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="bearerType" type="tns:Bearer" />
    </sequence>
  </complexType>

  <!-- allowed bearer type list -->
  <complexType name="AllowedBearerTypes">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="bearerType" type="tns:Bearer" />
    </sequence>
  </complexType>

  <!-- update imei alarm -->
  <complexType name="AlarmUpdateImeiCriteria">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="oldImei" type="string" />
      <element minOccurs="1" maxOccurs="1" name="lastOldImeiDate" type="dateTime" />
      <element minOccurs="1" maxOccurs="1" name="newImei" type="string" />
      <element minOccurs="1" maxOccurs="1" name="firstNewImeiDate" type="dateTime" />
    </sequence>
  </complexType>

  <!-- search alarms request input parameters -->
  <group name="SearchAlarmsIn">
    <sequence>
      <choice>
        <!-- search alarm by targets -->
        <element minOccurs="1" maxOccurs="1" name="searchByTargets" type="tns:SearchAlarmsByTargets" />
        <!-- search alarm by triggers -->
        <element minOccurs="1" maxOccurs="1" name="searchByTriggers" type="tns:SearchAlarmsByTriggers" />
      </choice>

      <element minOccurs="0" maxOccurs="1" name="beginDate" type="dateTime" />
      <element minOccurs="0" maxOccurs="1" name="endDate" type="dateTime" />
      <element minOccurs="0" maxOccurs="1" name="rangeStart" type="tns:RangeStart" />
      <element minOccurs="0" maxOccurs="1" name="rangeSize" type="tns:RangeSize" />
      <!-- show alarm status -->
      <element minOccurs="0" maxOccurs="1" name="showAlarmStatus" type="boolean" />
      <!-- show all origin traffic alarm -->
      <element minOccurs="0" maxOccurs="1" name="showAllOriginTrafficAlarms" type="boolean" />
      <!-- show machine traffic alarms -->
      <element minOccurs="0" maxOccurs="1" name="showMachineAlarms" type="boolean" />
      <!-- show extended triggers -->
      <element minOccurs="0" maxOccurs="1" name="showExtendedAlarms" type="boolean" />
      <!-- show M2M subscription status change triggers -->
      <element minOccurs="0" maxOccurs="1" name="showSubscriptionStatusChangeAlarms" type="boolean" />
      <!-- show Alarms on subscription option change -->
      <element minOccurs="0" maxOccurs="1" name="showSubscriptionOptionChangeAlarms" type="boolean" />
      <!-- show business rules -->
      <element minOccurs="0" maxOccurs="1" name="showBusinessRules" type="boolean" />	  
    </sequence>
  </group>

  <!-- range size -->
  <simpleType name="RangeSize">
    <restriction base="int">
      <minInclusive value="1" />
    </restriction>
  </simpleType>

  <!-- range start -->
  <simpleType name="RangeStart">
    <restriction base="int">
      <minInclusive value="0" />
    </restriction>
  </simpleType>

  <!-- search alarm by target -->
  <complexType name="SearchAlarmsByTargets">
    <sequence>
      <choice>
        <element minOccurs="1" maxOccurs="1" name="lineIdentifiers" type="tns:LineIdentifierCollection" />
        <element minOccurs="1" maxOccurs="1" name="customerEnvironmentNumber" type="tns:CustomerEnvironmentNumber" />
      </choice>
      <element minOccurs="0" maxOccurs="1" name="groupId" type="tns:GroupId" />
      <element minOccurs="0" maxOccurs="1" name="triggerType" type="tns:TriggerType" />
      <element minOccurs="0" maxOccurs="1" name="level" type="tns:Level" />
    </sequence>
  </complexType>

  <!-- search alarms by triggers identifiers -->
  <complexType name="SearchAlarmsByTriggers">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="triggerIdentifiers" type="tns:TriggerIdentifiers" />
    </sequence>
  </complexType>

  <!-- search alarms request input parameters -->
  <group name="SearchAlarmsOut">
    <sequence>
      <element minOccurs="0" maxOccurs="1" name="alarms" type="tns:Alarms" />
      <element minOccurs="0" maxOccurs="1" name="unknownLineIdentifiers" type="tns:LineIdentifierCollection" />
      <element minOccurs="0" maxOccurs="1" name="unknownTriggerIdentifiers" type="tns:TriggerIdentifiers" />
      <element minOccurs="1" maxOccurs="1" name="resultsNumberExceeded" type="boolean" />
      <element minOccurs="1" maxOccurs="1" name="totalResultsNumber" type="int" />
    </sequence>
  </group>

  <!-- unknown alarm identifier list -->
  <complexType name="UnknownAlarmIdentifiers">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="alarmIdentifier" type="tns:AlarmIdentifier" />
    </sequence>
  </complexType>

  <!-- trigger type -->
  <simpleType name="TriggerType">
    <restriction base="string">
      <enumeration value="TRAFFIC_TIMESTAMP" />
      <enumeration value="TRAFFIC_LOCATION" />
      <enumeration value="TRAFFIC_BEARER" />
      <enumeration value="TRAFFIC_UNITARY_THRESHOLD" />
      <enumeration value="TRAFFIC_GLOBAL_THRESHOLD" />
      <enumeration value="UPDATE_IMEI" />
      <enumeration value="TRAFFIC_LAZY_MACHINE" />
      <enumeration value="TRAFFIC_SILENT_MACHINE" />
      <enumeration value="TRAFFIC_CRAZY_MACHINE" />
      <enumeration value="M2M_SUBSCRIPTION_STATUS_TRIGGER" />
      <enumeration value="M2M_SUBSCRIPTION_OPTION_TRIGGER" />
    </restriction>
  </simpleType>


  <!-- GroupId -->
  <simpleType name="GroupId">
    <restriction base="int">
      <minInclusive value="0" />
    </restriction>
  </simpleType>

  <!-- Allowed periods -->
  <complexType name="AllowedPeriods">
    <complexContent>
      <extension base="tns:TwoPeriods">
        <attribute name="scale" type="tns:Scale" use="required" />
      </extension>
    </complexContent>
  </complexType>

  <!-- Two periods -->
  <complexType name="TwoPeriods">
    <sequence>
      <element minOccurs="1" maxOccurs="2" name="period" type="tns:Period" />
    </sequence>
  </complexType>

  <!-- period -->
  <complexType name="Period">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="begin" type="int" />
      <element minOccurs="1" maxOccurs="1" name="end" type="int" />
    </sequence>
  </complexType>

  <!-- period scale -->
  <simpleType name="Scale">
    <restriction base="string">
      <!-- hour -->
      <enumeration value="HOUR_PER_DAY" />
      <!-- day -->
      <enumeration value="DAY_PER_MONTH" />
    </restriction>
  </simpleType>

  <!-- notification level -->
  <simpleType name="Level">
    <restriction base="string">
      <enumeration value="HIGH" />
      <enumeration value="MEDIUM" />
      <enumeration value="LOW" />
    </restriction>
  </simpleType>

  <!-- trigger comment -->
  <simpleType name="TriggerComment">
    <restriction base="string">
      <maxLength value="128" />
    </restriction>
  </simpleType>

  <!-- Trigger status -->
  <simpleType name="TriggerStatus">
    <restriction base="string">
      <enumeration value="ENABLE" />
      <enumeration value="DISABLE" />
      <enumeration value="ALARM_SENT" />
    </restriction>
  </simpleType>

  <!-- Allowed countries and oprators -->
  <complexType name="AllowedCountriesAndOperators">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="country" type="tns:Country" />
    </sequence>
  </complexType>

  <!-- forbidden countries and oprators -->
  <complexType name="ForbiddenCountriesAndOperators">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="country" type="tns:Country" />
    </sequence>
  </complexType>

  <!-- msisdn type -->
  <complexType name="Msisdn">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="cc" type="string" />
      <element minOccurs="1" maxOccurs="1" name="sn" type="string" />
    </sequence>
  </complexType>

  <!-- Line Identifier -->
  <complexType name="LineIdentifier">
    <choice>
      <element minOccurs="1" maxOccurs="1" name="subscriptionNumber" type="string" />
      <element minOccurs="1" maxOccurs="1" name="simSerialNumber" type="string" />
      <element minOccurs="1" maxOccurs="1" name="deviceImei" type="string" />
      <element minOccurs="1" maxOccurs="1" name="deviceSerialNumber" type="string" />
      <element minOccurs="1" maxOccurs="1" name="deviceUniqueId" type="string" />
      <element minOccurs="1" maxOccurs="1" name="machineSerialNumber" type="string" />
      <element minOccurs="1" maxOccurs="1" name="msisdnVoice" type="tns:Msisdn" />
      <element minOccurs="1" maxOccurs="1" name="msisdnData" type="tns:Msisdn" />
      <element minOccurs="1" maxOccurs="1" name="msisdnFax" type="tns:Msisdn" />
    </choice>
  </complexType>

  <!-- Line Identifier Collection -->
  <complexType name="LineIdentifierCollection">
    <choice>
      <element minOccurs="1" maxOccurs="unbounded" name="subscriptionNumber" type="string" />
      <element minOccurs="1" maxOccurs="unbounded" name="simSerialNumber" type="string" />
      <element minOccurs="1" maxOccurs="unbounded" name="deviceImei" type="string" />
      <element minOccurs="1" maxOccurs="unbounded" name="deviceSerialNumber" type="string" />
      <element minOccurs="1" maxOccurs="unbounded" name="deviceUniqueId" type="string" />
      <element minOccurs="1" maxOccurs="unbounded" name="machineSerialNumber" type="string" />
      <element minOccurs="1" maxOccurs="unbounded" name="msisdnVoice" type="tns:Msisdn" />
      <element minOccurs="1" maxOccurs="unbounded" name="msisdnData" type="tns:Msisdn" />
      <element minOccurs="1" maxOccurs="unbounded" name="msisdnFax" type="tns:Msisdn" />
    </choice>
  </complexType>

  <!-- Customer environment number -->
  <simpleType name="CustomerEnvironmentNumber">
    <restriction base="string">
      <maxLength value="30" />
    </restriction>
  </simpleType>

  <!-- Trigger identifier -->
  <simpleType name="TriggerIdentifier">
    <restriction base="int" />
  </simpleType>

  <!-- Trigger identifier -->
  <simpleType name="AlarmIdentifier">
    <restriction base="int" />
  </simpleType>

  <!-- Trigger identifier -->
  <simpleType name="positiveLong">
    <restriction base="long">
      <minInclusive value="0" />
    </restriction>
  </simpleType>


  <!-- Unknown line identifier error -->
  <element name="UnknownLineIdentifierError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="lineIdentifier" type="tns:LineIdentifier" />
      </sequence>
    </complexType>
  </element>

  <element name="UnknownCustomerEnvironnementNumberError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="unknownCustomerEnvironnementNumber" type="tns:CustomerEnvironmentNumber" />
      </sequence>
    </complexType>
  </element>

  <!-- Invalid period error -->
  <element name="InvalidPeriodError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="message" type="string" />
      </sequence>
    </complexType>
  </element>

  <!-- Malima generic error -->
  <element name="MalimaError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="code" type="string" />
        <element minOccurs="1" maxOccurs="1" name="message" type="string" />
      </sequence>
    </complexType>
  </element>

  <element name="UnknownTriggerIdentifierError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="triggerIdentifier" type="tns:TriggerIdentifier" />
      </sequence>
    </complexType>
  </element>

  <!-- alarm error -->
  <element name="TriggerLimitExceededError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="triggerLimit" type="int" />
      </sequence>
    </complexType>
  </element>

  <!-- Trigger error -->
  <element name="TriggerError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="message" type="string" />
      </sequence>
    </complexType>
  </element>


  <!-- Occurs when the ws returns too many results -->
  <element name="TooManyResultsError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="maxAllowedResults" type="int" />
      </sequence>
    </complexType>
  </element>

  <!-- Occurs when the line identifiers number threshold is exceeded -->
  <element name="TooManyAlarmIdentifiersError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="maxAllowedAlarmIdentifiersNumber" type="int" />
      </sequence>
    </complexType>
  </element>

  <!-- alarm error -->
  <element name="AlarmError">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="1" name="message" type="string" />
      </sequence>
    </complexType>
  </element>


  <group name="ManageAlarmsIn">
    <sequence>
      <!-- alarm info -->
      <element minOccurs="1" maxOccurs="1" name="alarmInformations" type="tns:AlarmInformations" />
    </sequence>
  </group>


  <!-- Alarm informations -->
  <complexType name="AlarmInformations">
    <sequence>
      <element minOccurs="1" maxOccurs="unbounded" name="alarmInformation" type="tns:AlarmInformation" />
    </sequence>
  </complexType>

  <!-- Alarm informations -->
  <complexType name="AlarmInformation">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="alarmIdentifier" type="tns:AlarmIdentifier" />
      <element minOccurs="1" maxOccurs="1" name="alarmStatus" type="string" />
      <element minOccurs="0" maxOccurs="1" name="alarmStatusComment" type="tns:AlarmStatusComment" />
    </sequence>
  </complexType>

  <!-- alarm status -->
  <simpleType name="AlarmStatusComment">
    <restriction base="string">
      <maxLength value="150" />
    </restriction>
  </simpleType>

  <group name="ManageAlarmsOut">
    <sequence>
      <element minOccurs="0" maxOccurs="1" name="alarmIdentifiers" type="tns:AlarmIdentifiers" />
      <element minOccurs="0" maxOccurs="1" name="unknownAlarmIdentifiers" type="tns:UnknownAlarmIdentifiers" />
      <element minOccurs="0" maxOccurs="1" name="alarmIdentifiersWithUnknownStatus" type="tns:AlarmIdentifiers" />
    </sequence>
  </group>

  <!-- Bearer -->
  <simpleType name="Bearer">
    <restriction base="string">
      <enumeration value="FAX" />
      <enumeration value="FIXED_SERVICE_COMPLEMENT" />
      <enumeration value="GPRS_EDGE" />
      <enumeration value="MMS" />
      <enumeration value="MULTIMEDIA_PURCHASE" />
      <enumeration value="SMS" />
      <enumeration value="UMTS" />
      <enumeration value="UNKNOWN" />
      <enumeration value="VALUE_BASED_COMMUNICATION" />
      <enumeration value="VOICE" />
      <enumeration value="WIFI" />
    </restriction>
  </simpleType>
  
  <!-- CallOrigin -->
  <simpleType name="CallOrigin">
    <restriction base="string">
      <enumeration value="ROAMING" />
      <enumeration value="UNKNOWN" />
      <enumeration value="LOCAL" />
      <enumeration value="ALL_ORIGIN" />
    </restriction>
  </simpleType>


   <!-- BusinessRules -->
  <complexType name="BusinessRules">
    <sequence>
      <element minOccurs="0" maxOccurs="1" name="slmBusinessRule" type="tns:SlmBusinessRule" />
    </sequence>
  </complexType>

  <!-- SlmBusinessRule -->
  <complexType name="SlmBusinessRule">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="requestedStatus" type="string" />
    </sequence>
  </complexType>

  <!-- TriggeredBusinessRules -->
  <complexType name="TriggeredBusinessRules">
    <sequence>
      <element minOccurs="0" maxOccurs="1" name="triggeredSlmBusinessRule" type="tns:TriggeredSlmBusinessRule" />
    </sequence>
  </complexType>

  <!-- TriggeredSlmBusinessRule -->
  <complexType name="TriggeredSlmBusinessRule">
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="requestedStatus" type="string" />
      <element minOccurs="0" maxOccurs="1" name="taskStatus" type="string" />
      <element minOccurs="0" maxOccurs="1" name="errorCode" type="string" />
      <element minOccurs="0" maxOccurs="1" name="errorMessage" type="string" />
    </sequence>
  </complexType>

  
</schema>
